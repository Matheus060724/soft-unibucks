generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/mongo"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGO")
}

model user {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String?
  email     String   @unique
  CPF       String?
  telefone  String?
  createdAt DateTime @default(now())
}
